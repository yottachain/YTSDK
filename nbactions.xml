<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Dsun.net.useExclusiveBind=false -classpath %classpath com.ytfs.client.examples.SDKTest</exec.args>
                <exec.executable>java</exec.executable>
                <env.P2PHOST_CONNECTTIMEOUT>15000</env.P2PHOST_CONNECTTIMEOUT>
                <env.P2PHOST_READTIMEOUT>15000</env.P2PHOST_READTIMEOUT>
                
                
            </properties>
        </action>
        <action>
            <actionName>debug</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -Dsun.net.useExclusiveBind=false -classpath %classpath com.ytfs.client.examples.SDKTest</exec.args>
                <exec.executable>java</exec.executable>
                <jpda.listen>true</jpda.listen>
            </properties>
        </action>
        <action>
            <actionName>profile</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Dsun.net.useExclusiveBind=false -classpath %classpath com.ytfs.client.examples.SDKTest</exec.args>
                <exec.executable>java</exec.executable>
            </properties>
        </action>
    </actions>
